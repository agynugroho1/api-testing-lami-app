{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "string"
    },
    "currenttime": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "ratingID": {
              "type": "integer"
            },
            "image": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "rating": {
              "type": "integer"
            },
            "review": {
              "type": "string"
            }
          },
          "required": [
            "ratingID",
            "image",
            "name",
            "rating",
            "review"
          ]
        }
      ]
    },
    "message": {
      "type": "string"
    }
  },
  "required": [
    "code",
    "currenttime",
    "data",
    "message"
  ]
}