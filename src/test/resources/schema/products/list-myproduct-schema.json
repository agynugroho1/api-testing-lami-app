{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "string"
    },
    "currenttime": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "productID": {
              "type": "integer"
            },
            "image": {
              "type": "string"
            },
            "productName": {
              "type": "string"
            },
            "price": {
              "type": "integer"
            },
            "city": {
              "type": "string"
            }
          },
          "required": [
            "productID",
            "image",
            "productName",
            "price",
            "city"
          ]
        }
      ]
    },
    "message": {
      "type": "string"
    }
  },
  "required": [
    "code",
    "currenttime",
    "data",
    "message"
  ]
}