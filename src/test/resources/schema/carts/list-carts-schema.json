{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "code": {
      "type": "string"
    },
    "currenttime": {
      "type": "string"
    },
    "data": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "qty": {
              "type": "integer"
            },
            "url": {
              "type": "string"
            },
            "product_name": {
              "type": "string"
            },
            "price": {
              "type": "integer"
            }
          },
          "required": [
            "id",
            "qty",
            "url",
            "product_name",
            "price"
          ]
        }
      ]
    },
    "message": {
      "type": "string"
    }
  },
  "required": [
    "code",
    "currenttime",
    "data",
    "message"
  ]
}